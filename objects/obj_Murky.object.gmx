        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Player actions per step.

/* Get player input. */
key_right = keyboard_check(ord('D') ); // Equals NOT or 1
key_left = -keyboard_check(ord('A') ); // Equals NOT or -1.
key_jump = keyboard_check_pressed(vk_space);
/* Was it pressed the frame before? No? Do the action. Can't be held down.*/

/* React to input. */
move = key_left + key_right;
hori_speed = move * move_speed; // Move direction * initialized speed.
if (vert_speed &lt; 10) vert_speed += grav; // Limits terminal velocity.

/* Basic Jump Collision */
if (place_meeting(x, y + 1, obj_Grass) ) { // If on top of Grass.
    vert_speed = key_jump * - jump_speed;
}

if (place_meeting(x, y + 1, obj_RockEdge) ) { // If on top of Rock Edge.
    vert_speed = key_jump * - jump_speed;
}

/* Grass Horizontal Collision */
if (place_meeting(x + hori_speed, y, obj_Grass) ) { // Are we ABOUT to collide?
    while(!place_meeting(x + sign(hori_speed), y, obj_Grass) ) { // While NOT hitting wall...
        // Get as close as you can to the collision 1 pixel at a time.
        x += sign(hori_speed);
    }
    hori_speed = 0;
}
if (place_meeting(x + hori_speed, y, obj_RockEdge) ) {
    while(!place_meeting(x + sign(hori_speed), y, obj_RockEdge) ) {
        x += sign(hori_speed);
    }
    hori_speed = 0;
}
x += hori_speed; // Reset.

/* Vertical Collision */
if (place_meeting(x, y + vert_speed, obj_Grass) ) {
    while(!place_meeting(x, y + sign(vert_speed), obj_Grass) ) {
        y += sign(vert_speed);
    }
    vert_speed = 0;
}
if (place_meeting(x, y + vert_speed, obj_RockEdge) ) {
    while(!place_meeting(x, y + sign(vert_speed), obj_RockEdge) ) {
        y += sign(vert_speed);
    }
    vert_speed = 0;

}
y += vert_speed;
</string>
          </argument>
        </arguments>
      </action>
    </event>
